{
  "name": "scientia",
  "version": "1.0.0",
  "description": "Desktop Library Management System",
  "main": "main.js",
  "scripts": {
    "dev": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --no-asar --icon=icons/logo.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . scientia --overwrite --no-asar --platform=win32 --arch=ia32 --icon=icons/logo.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Scientia\"",
    "package-linux": "electron-packager . scientia --overwrite --no-asar --platform=linux --arch=x64 --icon=icons/logo.png --prune=true --out=release-builds"
  },
  "keywords": [
    "Electron",
    "php",
    "techsoul"
  ],
  "author": "Kel 3",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^13.1.0",
    "electron-packager": "^15.5.2"
  },
  "dependencies": {
    "php-server-manager": "^1.3.3"
  }
}
